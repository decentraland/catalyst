{
  "name": "@catalyst/content-server",
  "version": "0.1.0",
  "description": "Content server",
  "license": "Apache-2.0",
  "author": "Decentraland Contributors",
  "scripts": {
    "build": "tsc -b",
    "cleanup": "shx rm -rf dist node_modules",
    "report-api-coverage": "node ./reportApiCoverage.js",
    "start:db": "node ./dist/src/entrypoints/run-local-database.js",
    "start:maintenance": "node ./dist/src/entrypoints/run-maintenance.js",
    "start:server": "node ./dist/src/entrypoints/run-server.js",
    "test": "jest --runInBand --forceExit --detectOpenHandles --coverage",
    "test:integration": "jest --selectProjects integration --runInBand --forceExit --detectOpenHandles --coverage",
    "test:unit": "jest --selectProjects unit --runInBand --forceExit --detectOpenHandles --coverage"
  },
  "dependencies": {
    "@dcl/content-validator": "1.4.3",
    "@dcl/hashing": "^1.1.0",
    "@dcl/snapshots-fetcher": "3.0.2",
    "@well-known-components/interfaces": "^1.1.1",
    "@well-known-components/logger": "^1.1.0",
    "bloom-filters": "^3.0.0",
    "dcl-catalyst-client": "^11.3.0",
    "eth-crypto": "2.3.0",
    "multer": "^1.4.4",
    "node-cache": "^5.1.2",
    "node-pg-migrate": "6.2.1",
    "p-queue": "6.6.2",
    "pg": "8.7.3",
    "pg-promise": "10.11.1",
    "pg-query-stream": "^4.2.1",
    "sql-template-strings": "^2.2.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/destroy": "1.0.0",
    "@types/faker": "5.5.8",
    "@types/multer": "^1",
    "@types/on-finished": "2.3.1",
    "@types/pg": "^8",
    "@types/uuid": "^8",
    "chalk": "4.1.2",
    "columnify": "1.6.0",
    "destroy": "1.2.0",
    "faker": "5.5.3",
    "jest-extra-utils": "^0.1.0",
    "jest-junit": "^13.0.0",
    "on-finished": "2.4.1",
    "testcontainers": "8.9.0"
  }
}
